<launch>
  <node pkg="mx3_vision" name="object_tracker" type="object_tracker.py" output="screen">
    <remap from="camera_info" to="/camera/rgb/camera_info" />
    <rosparam>
       rate: 10
       max_rotation_speed: 0.2
       min_rotation_speed: 0.02
       x_threshold: 0.1
       gain: 0.1
    </rosparam>
  </node>
</launch>
